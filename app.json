
{
  "expo": {
    "name": "Recipe Reader",
    "description": "Finds a recipe & reads it to you while you cook!",
    "slug": "recipe-reader",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
    ],
    "version": "0.4.0",
    "orientation": "portrait",
    "primaryColor": "#F8C802",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f2f1ef"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "ios": {
      "bundleIdentifier": "com.gmail.abedolinger.recipereader"
    },
    "android": {
      "package": "com.gmail.abedolinger.recipereader"
    }
  }
}